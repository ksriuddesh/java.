import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
  
  Scanner sc = new Scanner(System.in);
  System.out.println("enter any number two know the factorial of taht number") ;
  int num = sc.nextInt() ;
  
  int mul = 1;
  for(int i =1;i<=num;i++){
    //   System.out.println(i) ;
      mul = mul*i;
  }
  System.out.println("FACTORS OF THAT NUMBERR IS "+  mul) ;
  
  
  
}
}
